package com.app.multimoduleappinitial.test;

import java.util.ArrayList;
import java.util.List;

public class AlgoAndDsClass {

	public static void main(String args[]) {

		findsubSequence("abc", "", 0); // Calling a function

	}

	public static void findsubSequence(String input, String output, int index) {

		// Base Case
		if (index >= input.length()) {
			for (int i = 0; i < output.length(); i++) {
				System.out.print(output.charAt(i));
			}
			System.out.println();
			return;
		}

		// including element
		output = output + input.charAt(index);
		findsubSequence(input, output, index + 1);

		// excluding element
		output = output.substring(0, output.length()-1 );

		findsubSequence(input, output, index + 1);
	}

}
